{
  "manifest_version": 2,
  "name": "Dev3 Toolkit",
  "version": "1.0.0",
  "description": "Four essential tools for power users and developers.",
  "icons": {
    "128": "./logo.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "history",
    "browsingData",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
